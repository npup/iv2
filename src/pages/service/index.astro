---
import { getEntry } from "astro:content";
import Layout from "~/layouts/Layout.astro";
import PageUpdateInfo from "~/components/PageUpdateInfo.astro";

const page = await getEntry("pages", "service");
const { Content } = await page.render();
---

<Layout title="Service" page="/service" metaDescr="service reparation renovering">
    <Content />
    <PageUpdateInfo date={page.data.editDate} />
</Layout>
