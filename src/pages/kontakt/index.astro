---
import { getEntry } from "astro:content";
import Layout from "~/layouts/Layout.astro";
import PageUpdateInfo from "~/components/PageUpdateInfo.astro";

const page = await getEntry("pages", "kontakt");
const { Content } = await page.render();
---

<Layout title="Kontakt" page="/kontakt" metaDescr="kontakt mail telefon adress">
    <Content />
    <PageUpdateInfo date={page.data.editDate} />
</Layout>
